@model IEnumerable<UASPAW_115.Models.FormMasuk>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NoPolisi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TanggalMasuk)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NoPolisi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TanggalMasuk)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdMasuk">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdMasuk">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdMasuk">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
